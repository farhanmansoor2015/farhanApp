<p id="notice"><%= notice %></p>

<h1>Listing Session S</h1>

<table>
  <thead>
    <tr>
      <th>Description</th>
      <th>Start</th>
      <th>End</th>
      <th>Status</th>
      <th>Program</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @session_s.each do |session_| %>
      <tr>
        <td><%= session_.description %></td>
        <td><%= session_.start %></td>
        <td><%= session_.end %></td>
        <td><%= session_.status %></td>
        <td><%= session_.program %></td>
        <td><%= link_to 'Show', session_ %></td>
        <td><%= link_to 'Edit', edit_session__path(session_) %></td>
        <td><%= link_to 'Destroy', session_, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Session ', new_session__path %>
