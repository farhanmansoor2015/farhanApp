<p id="notice"><%= notice %></p>

<h1>Listing Admissions</h1>

<table>
  <thead>
    <tr>
      <th>Firstname</th>
      <th>Lastname</th>
      <th>Registration</th>
      <th>Nic</th>
      <th>Address</th>
      <th>City</th>
      <th>Province</th>
      <th>District</th>
      <th>Lastdegree</th>
      <th>Degree</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @admissions.each do |admission| %>
      <tr>
        <td><%= admission.Firstname %></td>
        <td><%= admission.Lastname %></td>
        <td><%= admission.Registration %></td>
        <td><%= admission.Nic %></td>
        <td><%= admission.Address %></td>
        <td><%= admission.City %></td>
        <td><%= admission.Province %></td>
        <td><%= admission.District %></td>
        <td><%= admission.Lastdegree %></td>
        <td><%= admission.degree.Name%></td>
        <td><%= link_to 'Show', admission %></td>
        <td><%= link_to 'Edit', edit_admission_path(admission) %></td>
        <td><%= link_to 'Destroy', admission, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>
<div style="padding-left:45%; font-size:16px;">
<%= link_to 'New Admission', new_admission_path %>
